{
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "@object-ui/components": "file:../object-ui-package",
    "clsx": "^2.1.1",
    "preact": ">=10",
    "prismjs": "1.29.0",
    "react-icons": "^5.5.0",
    "react-simple-code-editor": "0.13.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "1.100.2",
    "@types/prismjs": "1.26.3",
    "prettier": "^3.6.2",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\""
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "react-editor",
    "name": "Object Linter",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
